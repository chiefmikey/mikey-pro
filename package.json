{
  "name": "mikey-pro",
  "version": "1.0.0",
  "description": "Mikey Pro",
  "scripts": {
    "fix": "eslint --fix '**/*'",
    "publish": "npm run publish-eslint && npm run publish-prettier && npm run publish-stylelint && npm run publish-mikey-pro",
    "publish-eslint": "cd ./style-guide/eslint-config && npm publish --access public",
    "publish-eslint-only": "npm run publish-eslint && npm run publish-mikey-pro",
    "publish-mikey-pro": "cd ./style-guide && npm publish --access public && npm run sleep",
    "publish-prettier": "cd ./style-guide/prettier-config && npm publish --access public",
    "publish-stylelint": "cd ./style-guide/stylelint-config && npm publish --access public",
    "sleep": "sleep 10 && npm install"
  },
  "devDependencies": {
    "mikey-pro": "^6.1.5"
  },
  "files": [
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://github.com/chiefmikey/mikey-pro#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chiefmikey/mikey-pro.git"
  },
  "bugs": {
    "url": "https://github.com/chiefmikey/mikey-pro/issues"
  },
  "license": "MIT",
  "keywords": [
    "mikey-pro",
    "mikey",
    "pro",
    "style guide",
    "style-guide",
    "lint",
    "linter",
    "format",
    "formatter"
  ],
  "author": "Mikl Wolfe <wolfe@mikl.io> (https://mikl.io)",
  "prettier": "@mikey-pro/prettier-config",
  "eslintConfig": {
    "extends": "@mikey-pro/eslint-config"
  },
  "stylelint": {
    "extends": "@mikey-pro/stylelint-config"
  }
}
